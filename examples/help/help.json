{
  "name": "BashBot Help",
  "description": "Show this message",
  "envvars": ["BASHBOT_CONFIG_FILEPATH"],
  "dependencies": ["jq"],
  "help": "bashbot help",
  "trigger": "help",
  "location": "./",
  "command": ["jq -r '.tools[] | \"\\(.help) - \\(.description)\"' ${BASHBOT_CONFIG_FILEPATH}"],
  "parameters": [],
  "log": false,
  "ephemeral": false,
  "response": "code",
  "permissions": [
    "all"
  ]
}